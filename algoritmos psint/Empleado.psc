Algoritmo Empleado
	Definir nombre Como Caracter
	Definir hora,vh,salario como entero 
	Escribir "digite el nombre del empleado";
	Leer nombre;
	Escribir "horas trabajadas";
	Leer hora;
	Escribir "valor de la hora";
	Leer vh;
	salario<-hora*vh
	Escribir "el empleado:" ,nombre, "tiene un valor de salario de:" , salario;
	
	
	
	
FinAlgoritmo
